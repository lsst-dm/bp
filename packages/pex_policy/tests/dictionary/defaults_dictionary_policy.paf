#<?cfg paf dictionary ?>  
target: types
definitions: {
    bool_set_count: {
        type: bool
        maxOccurs: 1
        allowed: { value: true }
        default: true
    }

    int_range_count: {
        type: int
        minOccurs: 2
        allowed: {
            min: -10
            max: -3
        }
        default: -5 -7 -6
    }

    deep: {
        type: Policy
        dictionary: {
            definitions: {
                sub_double: {
                    type: double
                    default: 1.
                }
            }
        }
    }

    indirect: {
        type: Policy
        dictionary: @defaults_dictionary_indirect.paf
    }

    # this is okay because no default is given -- eventually a value will
    # need to be supplied, but not necessarily right away.
    required: {
        minOccurs: 1
    }
}
